{
  "name": "konda/tourist-and-travellism",
  "description": "Travel AI Planning Backend with PHP, MongoDB, Cloudinary, and Google Gemini",
  "type": "project",
  "autoload": {
    "psr-4": {
      "Konda\\TouristAndTravellism\\": "src/",
      "TravelAI\\": "src/"
    }
  },
  "authors": [
    {
      "name": "PAMARTHILVSIVANAGESWARARAO",
      "email": "24a85a0506@sves.org.in"
    }
  ],
  "require": {
    "mongodb/mongodb": "^1.20",
    "php": ">=8.0",
    "cloudinary/cloudinary_php": "^2.11",
    "firebase/php-jwt": "^6.10",
    "vlucas/phpdotenv": "^5.6",
    "guzzlehttp/guzzle": "^7.8"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
